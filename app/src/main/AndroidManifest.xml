<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.guestaccount.germanshepherd">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:label="@string/title"/>
        <activity android:name=".InfoActivity1"
                  android:label="@string/title_info_activity1">
        </activity>
        <activity android:name=".InfoActivity2"
                  android:label="@string/title_info_activity2">
        </activity>
        <activity android:name=".InfoActivity3"
                  android:label="@string/title_info_activity3">
        </activity>
    </application>

</manifest>